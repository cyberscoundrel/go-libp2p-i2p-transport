# I2P Client Configuration
# Enable SAM bridge for libp2p transport

# SAM Bridge Configuration
clientApp.0.main=net.i2p.sam.SAMBridge
clientApp.0.name=SAMBridge
clientApp.0.args=sam.keys 0.0.0.0 7656 i2cp.tcp.host=127.0.0.1 i2cp.tcp.port=7654
clientApp.0.delay=1
clientApp.0.startOnLoad=true

# I2CP Configuration (required for SAM)
clientApp.1.main=net.i2p.i2ptunnel.I2PTunnel
clientApp.1.name=I2PTunnel
clientApp.1.args=-e config /var/lib/i2p/i2p-config/i2ptunnel.config
clientApp.1.delay=1
clientApp.1.startOnLoad=true

